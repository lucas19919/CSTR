{
  "openapi": "3.1.1",
  "info": {
    "title": "CSTR_MANAGER | v1",
    "version": "1.0.0"
  },
  "servers": [
    {
      "url": "https://localhost:7230"
    },
    {
      "url": "http://localhost:5053"
    }
  ],
  "paths": {
    "/Reactor/initialize": {
      "post": {
        "tags": [
          "Reactor"
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/CSTR_Reactor"
              }
            },
            "text/json": {
              "schema": {
                "$ref": "#/components/schemas/CSTR_Reactor"
              }
            },
            "application/*+json": {
              "schema": {
                "$ref": "#/components/schemas/CSTR_Reactor"
              }
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/Reactor/step": {
      "post": {
        "tags": [
          "Reactor"
        ],
        "parameters": [
          {
            "name": "dt",
            "in": "query",
            "schema": {
              "pattern": "^-?(?:0|[1-9]\\d*)(?:\\.\\d+)?(?:[eE][+-]?\\d+)?$",
              "type": [
                "number",
                "string"
              ],
              "format": "float",
              "default": 0.1
            }
          }
        ],
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/Reactor/status": {
      "get": {
        "tags": [
          "Reactor"
        ],
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/Reactor/simulate": {
      "get": {
        "tags": [
          "Reactor"
        ],
        "parameters": [
          {
            "name": "time",
            "in": "query",
            "schema": {
              "pattern": "^-?(?:0|[1-9]\\d*)$",
              "type": [
                "integer",
                "string"
              ],
              "format": "int32"
            }
          }
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/CSTR_Reactor"
              }
            },
            "text/json": {
              "schema": {
                "$ref": "#/components/schemas/CSTR_Reactor"
              }
            },
            "application/*+json": {
              "schema": {
                "$ref": "#/components/schemas/CSTR_Reactor"
              }
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    }
  },
